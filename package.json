{
    "name": "my-iclinic",
    "version": "1.0.0",
    "description": "Our specialist consultants offer a selection of laser eye surgery and lens surgery treatments that allow you to discover crystal clear vision.",
    "main": "index.js",
    "author": "https://digitaltechnologia.com",
    "license": "MIT",
    "scripts": {
        "dev": "webpack --config webpack.config.js --mode development",
        "tailwind": "chokidar \"**/*.php\" \"**/*.ts\" \"**/*.tsx\" -c \"tailwindcss -i ./src/styles/tailwind.css -o ./build/styles/tailwind.css\"",
        "build": "webpack --config webpack.config.js --mode production && tailwindcss -i ./src/styles/tailwind.css -o ./build/styles/tailwind.css --minify",
        "code-check": "npx eslint . --ext .ts --ext .tsx",
        "format": "npx prettier --write \"**/*.+(html|css|scss|sass|less|js|jsx|ts|tsx|json|php)\"",
        "tailwind-format": "shell-scripts/pretify-php.sh>/dev/null",
        "stylelint": "npx stylelint",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6",
        "@prettier/plugin-php": "^0.19.1",
        "@types/browser-sync-webpack-plugin": "^2.2.2",
        "@types/react-calendar": "^3.5.3",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "autoprefixer": "^10.4.12",
        "babel-loader": "^8.2.5",
        "browser-sync": "^2.27.10",
        "browser-sync-webpack-plugin": "^2.3.0",
        "chokidar-cli": "^3.0.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.25.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.3.0",
        "eslint-plugin-promise": "^6.0.1",
        "glob-all": "^3.3.0",
        "husky": "^8.0.0",
        "mini-css-extract-plugin": "^2.6.1",
        "postcss-lit": "^0.5.0",
        "postcss-loader": "^7.0.1",
        "postcss-preset-env": "^7.8.2",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "purgecss-webpack-plugin": "^4.1.3",
        "sass": "^1.55.0",
        "sass-loader": "^13.1.0",
        "style-loader": "^3.3.1",
        "tailwindcss": "^3.2.0",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {}
}
